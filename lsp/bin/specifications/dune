;; The .errors files are produced to show which typescript snippets failed to parse

(rule
 (alias runtest)
 (mode promote)
 (deps ./specification-3-15.md)
 (action
  (with-stdout-to
   3-15.errors
   (run ../gen.exe --out test --md %{deps}))))

(rule
 (alias runtest)
 (mode promote)
 (deps ./specification-3-16.md)
 (action
  (with-stdout-to
   3-16.errors
   (run ../gen.exe --out test --md %{deps}))))

